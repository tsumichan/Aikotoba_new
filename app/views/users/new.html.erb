<h1>新規登録</h1>
<%= form_for @user, :url => {:action => 'create'} do |f| %>
  <%#= render "shared/errors", object:@users  %>
  <%= f.label "ユーザー名" %>
  <%= f.text_field :user_name %>
  <%= f.label "パスワード" %>
  <%= f.password_field :password %>

  <%= f.submit "登録", class: "btn btn-primary"%>

<% end %>
